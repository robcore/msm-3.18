				config {
					pins = "gpio0", "gpio1", "gpio3"; /* MOSI, MISO, CLK */
					drive-strength = <12>; /* 12 MA */
					bias-disable = <0>; /* No PULL */
				};
			};
		};

		spi0_suspend {
			spi0_sleep: sleep {
				mux {
					pins = "gpio0", "gpio1", "gpio3"; /* MOSI, MISO, CLK */
					function = "gpio";
				};

				config {
					pins = "gpio0", "gpio1", "gpio3"; /* MOSI, MISO, CLK */
					drive-strength = <2>; /* 2 MA */
					bias-pull-down; /* pull down */
				};
			};
		};

		spi0_cs2_active {
			spi0_cs2_active: cs2_active {
				mux {
					pins = "gpio9"; /* CS */
					function = "blsp_spi1_cs2";
				};

				config {
					pins = "gpio9"; /* CS */
					drive-strength = <2>;
					bias-disable = <0>;
				};
			};
		};

		spi0_cs2_suspend {
			spi0_cs2_sleep: cs2_sleep {
				mux {
					pins = "gpio9"; /* CS */
					function = "gpio";
				};

				config {
					pins = "gpio9"; /* CS */
					drive-strength = <2>;
					bias-disable = <0>;
				};
			};
		};

		pcie0_clkreq {
			pcie0_clkreq_default: pcie0_clkreq_default {
				mux {
					pins = "gpio36";
					function = "pci_e1";
				};

				config {
					pins = "gpio36";
					drive-strength = <2>;
					bias-pull-up;					
				};
			};
		};

		pcie0_perst {
			pcie0_perst_default: pcie0_perst_default {
				mux {
					pins = "gpio35";
					function = "gpio";
				};

				config {
					pins = "gpio35";
					drive-strength = <2>;
					bias-pull-down;
				};
			};
		};

		pcie0_wake {
			pcie0_wake_default: pcie0_wake_default {
				mux {
					pins = "gpio37";
					function = "gpio";
				};

				config {
					pins = "gpio37";
					drive-strength = <2>;
					bias-pull-down;
				};
			};

			pcie0_wake_sleep: pcie0_wake_sleep {
				mux {
					pins = "gpio37";
					function = "gpio";
				};

				config {
					pins = "gpio37";
					drive-strength = <2>;
					bias-disable;
				};
			};
		};

		cnss_pins {
			cnss_default: default {
				mux {
					pins = "gpio112", "gpio54", "gpio53", "gpio113";
					function = "gpio";
				};

				config {
					pins = "gpio112", "gpio54", "gpio53", "gpio113";
					drive-strength = <16>;
					bias-pull-down;
				};
			};
		};

		pmx_i2c_2 {
			i2c_2_active: i2c_2_active {
				mux {
					pins = "gpio6", "gpio7"; /* SDA, SCL */
					function = "blsp_i2c2";
				};

				config {
					pins = "gpio6", "gpio7"; /* SDA, SCL */
					drive-strength = <2>;
					bias-disable;
				};
			};

			i2c_2_sleep: i2c_2_sleep {
				mux {
					pins = "gpio6", "gpio7"; /* SDA, SCL */
					function = "blsp_i2c2";
				};

				config {
					pins = "gpio6", "gpio7"; /* SDA, SCL */
					drive-strength = <2>;
					bias-disable;
				};
			};
		};
	};
};
